		"@testing-library/jest-dom": "^6.4.1",
		"@testing-library/svelte": "^4.1.0",
		"@types/mmmagic": "^0.4.33",
		"@types/uuid": "^9.0.6",
		"@vitest/coverage-v8": "^1.2.2",
		"autoprefixer": "^10.4.15",
		"daisyui": "^4.4.18",
		"postcss": "^8.4.28",
